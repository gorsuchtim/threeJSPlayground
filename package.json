{
  "name": "threejsplayground",
  "version": "1.1.0",
  "description": "Learning to build and playing with 3d animation with greensock and threeJS",
  "main": "src/index.htm",
  "scripts": {
    "autoprefix": "postcss dev/*.css -u autoprefixer -d build --map",
    "build": "npm run clean:build && npm run format && parcel build src/index.htm --out-dir build && npm run autoprefixer",
    "clean:build": "rimraf build",
    "clean:dev": "rimraf dev",
    "dev": "parcel src/index.htm --out-dir dev",
    "format": "npm run lint && npm run prettier",
    "prettier": "prettier --write \"src/**/*.{js,jsx,css,htm,json}\"",
    "prettier:check": "prettier --list-different \"src/**/*.{js,jsx,css,htm,json}\"",
    "lint": "eslint \"src/js/**/*.{js,jsx}\""
  },
  "author": "Tim Gorsuch",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "parcel-bundler": "^1.12.3",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "sass": "^1.23.0"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gsap": "^3.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "three": "^0.137.0"
  }
}
